        <label for="emailSignup">Email</label>
        <input type="text" name="emailSignup" id="emailSignup">
    </div>
    <div>
        <label for="firstName">First Name</label>
        <input type="text" name="firstName" id="firstName">
    </div>
    <div>
        <label for="lastName">First Name</label>
